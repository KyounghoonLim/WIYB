{
  "fade-in": {
    "0%": { "opacity": 0 },
    "100%": { "opacity": 1 }
  },
  "fade-out": {
    "0%": { "opacity": 1 },
    "100%": { "opacity": 0 }
  },
  "fade-in-out": {
    "0%": { "opacity": 0 },
    "10%": { "opacity": 1 },
    "90%": { "opacity": 1 },
    "100%": { "opacity": 0 }
  },
  "slide-in-horizontal-ltr": {
    "from": { "transform": "translateX(-100%)" },
    "to": { "transform": "translateX(0)" }
  },
  "slide-out-horizontal-ltr": {
    "from": { "transform": "translateX(0)" },
    "to": { "transform": "translateX(-100%)" }
  },
  "slide-in-horizontal-rtl": {
    "from": { "transform": "translateX(100%)" },
    "to": { "transform": "translateX(0)" }
  },
  "slide-out-horizontal-rtl": {
    "from": { "transform": "translateX(0)" },
    "to": { "transform": "translateX(100%)" }
  },
  "slide-in-vertical-ltr": {
    "from": { "transform": "translateY(-100%)" },
    "to": { "transform": "translateY(0)" }
  },
  "slide-out-vertical-ltr": {
    "from": { "transform": "translateY(0)" },
    "to": { "transform": "translateY(-100%)" }
  },
  "slide-in-vertical-rtl": {
    "from": { "transform": "translateY(100%)" },
    "to": { "transform": "translateY(0)" }
  },
  "slide-out-vertical-rtl": {
    "from": { "transform": "translateY(0)" },
    "to": { "transform": "translateY(100%)" }
  },
  "spin-arrow": {
    "from": { "transform": "rotate(0deg)" },
    "to": { "transform": "rotate(180deg)" }
  },
  "spin-arrow-reverse": {
    "from": { "transform": "rotate(180deg)" },
    "to": { "transform": "rotate(0deg)" }
  },
  "drop-options": {
    "from": { "transform": "translateY(-110%)" },
    "to": { "transform": "translateY(0)" }
  },
  "drop-options-reverse": {
    "from": { "transform": "translateY(0)" },
    "to": { "transform": "translateY(-110%)" }
  },
  "bottomsheet-open": {
    "from": { "transform": "translateY(100%)" },
    "to": { "transform": "translateY(0)" }
  },
  "bottomsheet-close": {
    "from": { "transform": "translateY(0)" },
    "to": { "transform": "translateY(100%)" }
  },
  "shimmer": {
    "100%": {
      "transform": "translateX(100%)"
    }
  }
}
