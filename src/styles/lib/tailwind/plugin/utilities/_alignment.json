{
  ".flex-row-center": {
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  ".flex-row-start": {
    "display": "flex",
    "justifyContent": "start",
    "alignItems": "center"
  },
  ".flex-col-center": {
    "display": "flex",
    "flex-direction": "column",
    "justifyContent": "center",
    "alignItems": "center"
  },
  ".flex-col-start": {
    "display": "flex",
    "flex-direction": "column",
    "justifyContent": "start",
    "alignItems": "center"
  }
}