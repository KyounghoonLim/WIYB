{
  ".CONTAINER": {
    "width": "100vw",
    "minWidth": "320px",
    "minHeight": "480px",
    "position": "relative",
    "overflowX": "hidden",
    "overflowY": "auto",
    "@apply h-dscreen": true,
    "@apply flex-col-start": true
  },
  ".CONTENT-CONTAINER": {
    "width": "100%",
    "minHeight": "100%",
    "maxWidth": "1024px",
    "margin": "0 auto",
    "padding": "44px 16px 16px",
    "position": "relative",
    "display": "flex",
    "flexDirection": "column"
  },
  ".SCROLLABLE-CONTAINER": {
    "width": "100%",
    "maxWidth": "1024px",
    "margin": "0 auto",
    "padding": "44px 16px 16px",
    "position": "relative",
    "display": "flex",
    "flexDirection": "column"
  },
  ".ISLAND-CONTAINER": {
    "width": "100%",
    "padding": "24px",
    "borderRadius": "20px",
    "@apply bg-@-neutral-000": true
  },
  ".SEARCH-CONTAINER": {
    "width": "100%",
    "height": "-webkit-fill-available",
    "display": "flex",
    "flexDirection": "column",
    "padding": "0px",
    "position": "fixed",
    "top": "0px",
    "left": "0px",
    "overscrollBehavior": "none",
    "@apply bg-@-neutral-000": true
  }
}
