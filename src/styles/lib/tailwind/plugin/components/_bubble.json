{
  ".bubble": {
    "padding": "8px 12px",
    "borderRadius": "20px"
  },
  ".bubble-chat": {
    "maxWidth": "85%",
    "@apply bubble": true,
    "@apply typograph-15": true
  },
  ".bubble-image": {
    "@apply bubble": true,
    "width": "120px",
    "height": "120px",
    "padding": "0",
    "overflow": "hidden"
  }
}