{
  ".category": {
    "width": "100%",
    "height": "44px",
    "gap": "8px",
    "@apply flex-row-start": true
  },
  ".category-item": {
    "height": "44px",
    "padding": "13.5px 12px",
    "cursor": "pointer",
    "@apply typograph-14": true,
    "@apply text-@-text-label": true,
    "&[data-selected='true']": {
      "fontWeight": "bold",
      "borderBottom": "2px solid black",
      "@apply text-@-neutral-900": true
    }
  }
}
