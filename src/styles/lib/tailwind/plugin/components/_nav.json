{
  ".NAV": {
    "width": "100%",
    "height": "50px",
    "margin": "0 auto",
    "padding": "0",
    "display": "flex",
    "justifyContent": "space-between",
    "alignItems": "center",
    "fontWeight": "700",
    "backgroundColor": "white",
    "@apply no-auto-size": true,
    "@apply z-nav": true,
    "& svg": {
      "cursor": "pointer"
    }
  }
}
