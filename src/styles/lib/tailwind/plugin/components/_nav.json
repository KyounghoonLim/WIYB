{
  ".NAV": {
    "width": "100%",
    "height": "44px",
    "minWidth": "320px",
    "maxWidth": "420px",
    "margin": "0 auto",
    "padding": "0 8px",
    "position": "absolute",
    "top": "0px",
    "display": "flex",
    "justifyContent": "space-between",
    "alignItems": "center",
    "fontWeight": "700",
    "@apply z-nav": true,
    "@apply typograph-16": true,
    "@apply text-@-neutral-900": true,
    "& svg": {
      "cursor": "pointer"
    },
    "&:empty": {
      "display": "none"
    }
  }
}
