{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "types/*": ["./src/@types/*"],
      "constants/*": ["./src/constants/*"],
      "components/*": ["./src/components/*"],
      "providers/*": ["./src/providers/*"],
      "services/*": ["./src/services/*"],
      "hooks/*": ["./src/hooks/*"],
      "utils/*": ["./src/utils/*"],
      "icons/*": ["./public/icons/*"],
      "images/*": ["./public/images/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "custom-next-env.d.ts",
    "svgr.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["next-env.d.ts", "node_modules"]
}
